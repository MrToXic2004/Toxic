import java.util.Scanner;

public class FourthClass {   //super class
    
    public int attributeX = 10;
    private int attributeY = 20;
        int attributeZ = 30;
    protected int attributeK = 40;

    public void methodOne(){
        System.out.println("Hi! methodOne");
    }
    public void methodTwo(){
        System.out.println("Hi! methodTwo");
    }
}

public class FifthClass extends FourthClass {   //sub class
    public static void main(String[] arge){
        System.out.println("main.FifthClass");
        FifthClass obj = new FifthClass();
        obj.methodOne();
    }
}
